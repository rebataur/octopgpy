	CREATE OR replace FUNCTION testspeed () 
	RETURNS numeric
	AS $$
	import numpy as np
	import numba
	from numba import jit,njit
	import random
	@njit
	def monte_carlo_pi(nsamples):
	    acc = 0
	    for i in range(nsamples):
	        x = random.random()
	        y = random.random()
	        if (x ** 2 + y ** 2) < 1.0:
	            acc += 1
	    return 4.0 * acc / nsamples
	
	return monte_carlo_pi(100)
